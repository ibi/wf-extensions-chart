	"properties": {
		"inverseData": false,
		"chordSort": "none",
		"groupPadding": 0.05,
		"groupCurves": {
			"radius": {
				"inner": "auto",
				"outer": "auto"
			},
			"title": {
				"font": "12px sans-serif",
				"color": "black",
				"bold": true
			}
		},
		"axis": {
			"color": "black",
			"preciseCount": false,
			"label": {
				"color": "black",
				"fontFamily": "sans-serif",
				"fontSize": "10px",
				"format": ".2s",
				"count": 20
			},
			"ticks": {
				"color": "black",
				"count": 90
			}
		}
	},
	"propertyAnnotations": {
		"inverseData": "bool",
		"chordSort": "str",
		"groupPadding": "number",
		"groupCurves": {
			"radius": {
				"inner": "str",
				"outer": "str"
			},
			"title": {
				"font": "str",
				"color": "color",
				"bold": "bool"
			}
		},
		"axis": {
			"color": "color",
			"preciseCount": "bool",
			"label": {
				"color": "color",
				"fontFamily": "str",
