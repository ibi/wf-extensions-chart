        "nodeAlign": "justify"
	},
	
	"propertyAnnotations": {
		"chartHeadroom": "number",
        "nodeWidth": {
				"typeAnnotation": "number",
				"numericRange": [5, 50],
				"numericStep": 1
        },
        "nodePadding": {
				"typeAnnotation": "number",
				"numericRange": [5, 50],
				"numericStep": 1
        },
        "colorMode": {
                "typeAnnotation": "str",
                "stringValues": ["none", "source", "target", "gradient"]
        },
        "colors": {
                "typeAnnotation": "array",
                "arrayTypes": "color"
        },
        "fadeOpacity": {
				"typeAnnotation": "number",
				"numericRange": [0.05, 0.2],
				"numericStep": .01
        },
        "nodeAlign": {
                "typeAnnotation": "str",
                "stringValues": ["justify", "left", "right", "center"]
        }
	},
	
	"dataBuckets":  {
	
		"buckets": [
			{
				"id": "source",
				"type": "dimension",
				"count": {
